{
  "name": "lens-hotkeys",
  "description": "Add some hotkeys to the Google Lens interface",
  "version": "1.0.0",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
    {
      "matches": [
        "https://lens.google.com/*"
      ],
      "js": [ "content.js" ]
    }
  ],
  "web_accessible_resources": [
    "content.js",
    "images/*",
    "fonts/*"
  ]
}