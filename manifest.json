{
  "name": "lens-hotkeys",
  "description": "Add some hotkeys to the Google Lens interface",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "https://lens.google.com/*"
      ],
      "js": [ "content.js" ],
      "run_at": "document_start"
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}